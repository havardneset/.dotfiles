# DI Login aliases

alias di="di-cli"
alias cdev="di aa 2 1 1 && eval \"\$(pbpaste)\""
alias cstg="di aa 2 2 1 && eval \"\$(pbpaste)\""
alias cprod="di aa 2 3 1 && eval \"\$(pbpaste)\""
alias fdev="di aa 1 1 1 && eval \"\$(pbpaste)\""
alias fstg="di aa 1 2 1 && eval \"\$(pbpaste)\""
alias fprod="di aa 1 3 1 &&eval \"\$(pbpaste)\"" 

alias vim="nvim"
alias n="nvim"

